#: ../src/predict_number.c:59
#, c-format
msgid "Guesses a number from 1 to 100 with support for Roman numbers.\n"
msgstr "Угадывает число от 1 до 100 с поддержкой римских цифр.\n"

#: ../src/predict_number.c:60
#, c-format
msgid "usage: number [-r]\n"
msgstr "Использование: number [-r]\n"

#: ../src/predict_number.c:61
#, c-format
msgid "\t-r\tEnable Roman numbers\n"
msgstr "Включает поддержку римских цифр\n"

#: ../src/predict_number.c:75
#, c-format
msgid "Guess a number from %s to %s.\n"
msgstr "Загадайте число от %s до %s.\n"

#: ../src/predict_number.c:80
msgid "yes"
msgstr "да"

#: ../src/predict_number.c:81
msgid "no"
msgstr "нет"

#: ../src/predict_number.c:88
#, c-format
msgid "Is the number greater than %s (yes or no)?\n"
msgstr "Загаданное число больше %s (да или нет)?\n"

#: ../src/predict_number.c:98
#, c-format
msgid "Wrong answer\n"
msgstr "Неправильный ответ\n"

#: ../src/predict_number.c:106
#, c-format
msgid "The correct number is %s\n"
msgstr "Правильное число %s\n"
